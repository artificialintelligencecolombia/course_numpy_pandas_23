   "source": [
    "# trnaspose is a method for reshaping the array without copying it\n",
    "arr = np.arange(15).reshape(3,5)\n",
    "print('array: \\n', arr)\n",
    "print('array.T: \\n', arr.T)\n",
    "dot = np.dot(arr, arr.T)\n",
    "print('dot product: \\n', dot)"
   ]
